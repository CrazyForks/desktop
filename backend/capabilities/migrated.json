{
  "identifier": "migrated",
  "description": "permissions that were migrated from v1",
  "context": "local",
  "windows": ["main"],
  "permissions": [
    "core:path:default",
    "core:event:default",
    "core:window:default",
    "core:app:default",
    "core:resources:default",
    "core:menu:default",
    "core:tray:default",
    "shell:allow-open",
    "sql:allow-load",
    "sql:allow-execute",
    "sql:allow-select",
    "os:allow-platform",
    "core:window:allow-start-dragging",
    {
      "identifier": "http:default",
      "allow": ["https://api.atuin.sh/*"]
    },
    "os:default",
    "dialog:default",
    "updater:default",
    "process:default",
    "process:allow-restart"
  ],
  "platforms": ["linux", "macOS", "windows"]
}
