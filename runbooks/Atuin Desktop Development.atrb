    text: 'You can start the app with another profile by setting a profile here or launching '
    styles: {}
  - type: text
    text: ./script/dev
    styles:
      code: true
  - type: text
    text: ' with the '
    styles: {}
  - type: text
    text: -p <profilename>
    styles:
      code: true
  - type: text
    text: ' flag. The default is “dev”. Passing '
    styles: {}
  - type: text
    text: -d
    styles:
      code: true
  - type: text
    text: ' starts the app with devtools open.'
    styles: {}
  children: []
